      <xs:element name="version"               type="xs:string"            minOccurs="1"  maxOccurs="1"                 />
      <xs:element name="versionXSD"            type="xs:string"            minOccurs="1"  maxOccurs="1"                 />
      <xs:element name="comment"               type="xs:string"            minOccurs="0"  maxOccurs="1"                 />
    </xs:sequence>                                                                                             
  </xs:complexType>
  
  <xs:complexType name="practiceType">
    <xs:sequence>
      <xs:element name="practiceID"            type="xs:string"            minOccurs="1"  maxOccurs="1"                 />
      <xs:element name="pmsName"               type="xs:string"            minOccurs="1"  maxOccurs="1"                 />
      <xs:element name="practiceName"          type="xs:string"            minOccurs="0"  maxOccurs="1"                 />
      <xs:element name="address"               type="xs:string"            minOccurs="0"  maxOccurs="1"                 />
      <xs:element name="deleteInd"             type="xs:string"            minOccurs="0"  maxOccurs="1"                 />      
    </xs:sequence>
  </xs:complexType>                                                                                            
                                                                                                                          
